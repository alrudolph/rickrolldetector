{
    "manifest_version": 2,
    "name": "Rick Roll Detector",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "web_accessible_resources": ["rick.jpg"],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_title": "Rick Roll Detector"
    },
    "permissions": ["storage"]
}